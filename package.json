{
  "name": "@processmaker/modeler",
  "version": "0.17.0",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "open-cypress": "TZ=UTC cypress open",
    "lint": "vue-cli-service lint --no-fix",
    "build-bundle": "vue-cli-service build --target lib --name modeler ./src/components/nodes/index.js",
    "bundle-analyze": "npm run build-bundle -- --analyze",
    "bundle-link": "npm run build-bundle",
    "postbundle-link": "npm link",
    "lint-fix": "vue-cli-service lint",
    "pretest": "rm -rf .nyc_output || true",
    "test": "npm run test-unit && npm run test-ci",
    "posttest": "npm run report:combined",
    "test-unit": "vue-cli-service test:unit",
    "test-ci": "TZ=UTC vue-cli-service test:e2e --mode test --headless --browser chrome",
    "mkdir:reports": "mkdir reports || true",
    "precopy:reports": "npm run mkdir:reports",
    "copy:reports": "cp cypress-coverage/coverage-final.json reports/from-cypress.json && cp jest-coverage/coverage-final.json reports/from-jest.json",
    "precombine:reports": "npm run copy:reports && mkdir .nyc_output || true",
    "combine:reports": "nyc merge reports && mv coverage.json .nyc_output/out.json",
    "prereport:combined": "npm run combine:reports",
    "report:combined": "nyc report --reporter lcov --report-dir coverage"
  },
  "main": "./dist/modeler.common.js",
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js",
    "blank.bpmn"
  ],
  "dependencies": {
    "@babel/plugin-proposal-private-methods": "^7.6.0",
    "@fortawesome/fontawesome-free": "^5.7.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.6",
    "@fortawesome/free-solid-svg-icons": "^5.6.1",
    "@fortawesome/vue-fontawesome": "^0.1.1",
    "bpmn-moddle": "^5.1.5",
    "bpmnlint": "^5.0.0",
    "bpmnlint-plugin-processmaker": "1.0.2",
    "file-saver": "^2.0.0-rc.4",
    "jointjs": "^3.0.4",
    "lodash": "^4.17.11",
    "luxon": "^1.11.4",
    "vue": "^2.5.17",
    "vue-deepset": "^0.6.3",
    "vue-upload-component": "^2.8.14",
    "vuex": "^3.0.1",
    "webpack": "latest"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^1.9.0",
    "@panter/vue-i18next": "^0.15.0",
    "@processmaker/processmaker-bpmn-moddle": "0.4.4",
    "@processmaker/screen-builder": "0.20.4",
    "@processmaker/vue-form-elements": "0.12.3",
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-e2e-cypress": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-plugin-unit-jest": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "axios": "^0.18.0",
    "axios-mock-adapter": "^1.16.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-istanbul": "^5.2.0",
    "bootstrap": "^4.3.1",
    "bootstrap-vue": "^2.0.0-rc.21",
    "bpmnlint-loader": "^0.1.3",
    "cypress": "^3.4.1",
    "cypress-vue-unit-test": "^1.11.0",
    "i18next": "^15.0.9",
    "istanbul-lib-coverage": "^2.0.5",
    "node-sass": "^4.9.4",
    "nyc": "^14.1.1",
    "sass-loader": "^7.0.1",
    "stylelint": "^9.7.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-scss": "^3.3.2",
    "vue-loader": "latest",
    "vue-template-compiler": "^2.5.17",
    "webpack-bundle-analyzer": "^3.5.2",
    "webpack-cli": "^3.3.9",
    "yargs": "^14.2.0"
  },
  "peerDependencies": {
    "vue": "^2.5.17",
    "bootstrap": "^4.3.1",
    "bootstrap-vue": "^2.0.0-rc.21",
    "@processmaker/screen-builder": "0.20.4",
    "@processmaker/vue-form-elements": "0.12.3",
    "@fortawesome/fontawesome-free": "^5.7.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.6",
    "@fortawesome/free-solid-svg-icons": "^5.6.1",
    "@fortawesome/vue-fontawesome": "^0.1.1",
    "jointjs": "^3.0.4",
    "i18next": "^15.0.9",
    "@panter/vue-i18next": "^0.15.0",
    "luxon": "^1.11.4",
    "lodash": "^4.17.11",
    "bpmn-moddle": "^5.1.5"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 2%",
    "last 2 versions",
    "not ie <= 8",
    "chrome >= 69"
  ],
  "nyc": {
    "extension": [
      ".js",
      ".vue"
    ],
    "exclude": [
      "tests/**/*.js"
    ],
    "report-dir": "cypress-coverage"
  }
}
